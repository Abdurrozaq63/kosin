{
  "prettier.requireConfig": false,
  "prettier.useEditorConfig": false,
  "workbench.colorTheme": "Tinacious Design",
  "workbench.iconTheme": "vscode-icons",
  "workbench.preferredDarkColorTheme": "Galactic Night",
  "jupyter.themeMatplotlibPlots": false,
  "editor.semanticHighlighting.enabled": "configuredByTheme",
  "workbench.activityBar.iconClickBehavior": "toggle",
  "editor.cursorStyle": "line",
  "editor.hideCursorInOverviewRuler": false,
  "editor.multiCursorMergeOverlapping": false,
  "editor.overtypeCursorStyle": "line",
  "scss.hover.references": false,
  "editor.hover.above": false,
  "editor.hover.enabled": false,
  "editor.hover.sticky": false,
  "C_Cpp.markdownInComments": "disabled",
  "accessibility.verbosity.hover": false,
  "typescript.tsserver.web.projectWideIntellisense.enabled": false,
  "editor.parameterHints.enabled": false
}
